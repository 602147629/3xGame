		public static function getMediatorName(id:String):String
		{
			return "Mediator" + id;
		}
		
		public static function getUIName(id:String):String
		{
			return id + "UI";
		}
		
		public static function getVerticalAlignInfomation(id:String):int
		{
			switch(id)
			{
				case ConstantUI.BOTTOM_FUNCTION_UI:
				case ConstantUI.TASK_SCRIPT:
				case ConstantUI.CHAT_PANEL:
				case ConstantUI.RAPID_EQUIP_UI:
					return ALIGN_VERTIVAL_BOTTOM;
				case ConstantUI.MAIN_PLAYER_PROPERTY_UI:
				case ConstantUI.TOP_FUNCTION_PANEL_UI:
				case ConstantUI.SYSTEM_BTN:
				case ConstantUI.RIGHT_TOP_UI:
				case ConstantUI.TODAYS_ACTIVE_UI:
				case ConstantUI.PARTNER_AVATAR_UI:
					return ALIGN_VERTIVAL_TOP;
				default:
					return ALIGN_VERTIVAL_TOP;
			}
		}
		
		public static function getHorizontalAlignInfomation(id:String):int
		{
			switch(id)
			{
				case ConstantUI.BOTTOM_FUNCTION_UI:
				case ConstantUI.BACK_TO_MAIN_CITY:
				case ConstantUI.TASK_TEXT_LIST:
				case ConstantUI.SYSTEM_BTN:
				case ConstantUI.RIGHT_TOP_UI:
				case ConstantUI.RAPID_EQUIP_UI:
					return ALIGN_HORIZONTAL_RIGHT;
				case ConstantUI.MAIN_PLAYER_PROPERTY_UI:
				case ConstantUI.TOP_FUNCTION_PANEL_UI:
					return ALIGN_HORIZONTAL_LEFT;
				default:
					return ALIGN_HORIZONTAL_LEFT;
			}
		}
		
		public static  function getMultiPanelAlign(id:String):int
		{
			if(id == ConstantUI.ROLE_PANEL)
			{
				return ALIGN_HORIZONTAL_LEFT;
			}else if(id == ConstantUI.DISABLE_MSG_UI)
			{
				return ALIGN_HORIZONTAL_NORMAL;
			}
			else if(id == ConstantUI.CONFIRM_PANEL)
			{
				return ALIGN_HORIZONTAL_NORMAL;
			}
			else if(id == ConstantUI.MONEYTREE_UI)
			{
				return ALIGN_HORIZONTAL_NORMAL;
			}
			else if(id == ConstantUI.EQUIP_STORE_PANEL_UI)
			{
				return ALIGN_HORIZONTAL_LEFT;
			}
			else if(id == ConstantUI.STAGE_CLEAR_UI)
			{
				return ALIGN_HORIZONTAL_LEFT;
			}
			else
			{
				return ALIGN_HORIZONTAL_RIGHT;
			}
		}
